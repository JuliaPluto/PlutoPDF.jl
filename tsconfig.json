{
    "compilerOptions": {
        "target": "ES2021",
        "moduleResolution": "Node",
        "lib": ["ES2021", "DOM", "WebWorker", "DOM.Iterable"],
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "strict": false,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "skipDefaultLibCheck": true
    }
}
